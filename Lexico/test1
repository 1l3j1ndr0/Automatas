%option noyywrap
%{
    #include <stdio.h>
%}

%%

[0-9]+             { printf("Número entero: %s\n", yytext); }
.                  { printf("Carácter no reconocido: %s\n", yytext); }

%%

int main() {
    printf("Ingrese texto para analizar:\n");
    yylex();  // Llama al analizador léxico
    return 0;
}